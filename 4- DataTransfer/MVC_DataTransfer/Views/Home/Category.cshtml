
@{
    ViewBag.Title = "Category";
}

<div class="row" style="margin-top:50px">
    <div class="panel panel-default">
        <div class="panel-body">
            <div class="col-lg-12">
                <h3 class="text-center">TempData</h3>
                <div class="panel panel-default">
                    <div class="panel-body">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Kategori Id</th>
                                    <th>Kategori Adı</th>
                                    <th>Açıklama</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{  foreach (var item in TempData["KategoriListesi"] as IEnumerable<MVC_DataTransfer.Models.Kategori>)
                                    {
                                        <tr>
                                            <td>@item.Id</td>
                                            <td>@item.KategoriAdi</td>
                                            <td>@item.Aciklama</td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div> 